{
  "name": "port-forwarder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^1.6.8",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primereact": "^10.6.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start:app": "node ./build.js app",
    "start:gui": "node ./build.js gui",
    "build": "node ./build.js build-all",
    "build:app": "node ./build.js build-app",
    "build:gui": "node ./build.js build-gui",
    "lint": "eslint ./build.js ./src",
    "lint:fix": "eslint ./build.js ./src --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "plugins": [
      "@stylistic/js"
    ],
    "rules": {
      "@stylistic/js/semi": ["error", "always"],
      "@stylistic/js/quotes": ["error", "single"],
      "@stylistic/js/no-tabs": ["error"],
      "@stylistic/js/wrap-iife": ["error", "inside"],
      "@stylistic/js/arrow-spacing": ["error", { "before": true, "after": true }],
      "@stylistic/js/keyword-spacing": ["error", { "before": true, "after": true }],
      "@stylistic/js/linebreak-style": ["error", "unix"],
      "@stylistic/js/space-in-parens": ["error", "always"],
      "@stylistic/js/space-infix-ops": ["error"],
      "@stylistic/js/space-unary-ops": ["error"],
      "@stylistic/js/space-before-blocks": ["error"],
      "@stylistic/js/rest-spread-spacing": ["error", "never"],
      "@stylistic/js/space-before-function-paren": ["error", { "named": "never", "anonymous": "never", "asyncArrow": "always" }]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "^2.0.0"
  }
}
